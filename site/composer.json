{
	"name" : "librecores/web",
	"license" : "MIT",
	"type" : "project",
	"description" : "LibreCores Web Site",
	"autoload" : {
        "psr-4": { "": "src/" },
        "files": ["app/AppKernel.php"]
	},
	"require" : {
		"php" : ">=7.1",
		"symfony/symfony" : "~3.2.0",
		"symfony/console" : "~3.2.3",
		"symfony/process" : "~3.2.3",
		"symfony/assetic-bundle" : "~2.8.1",
		"symfony/swiftmailer-bundle" : "~2.4.2",
		"symfony/monolog-bundle" : "~3.0.3",
		"doctrine/orm" : "~2.5.6",
		"doctrine/dbal" : "~2.5.12",
		"doctrine/doctrine-bundle" : "~1.6.7",
		"doctrine/doctrine-fixtures-bundle" : "<2.5",
		"twig/extensions" : "~1.0",
		"sensio/distribution-bundle" : "~5.0.18",
		"sensio/framework-extra-bundle" : "~3.0,>=3.0.2",
		"incenteev/composer-parameter-handler" : "~2.0",
		"knplabs/knp-markdown-bundle" : "~1.3",
		"fkr/simplepie-bundle" : "dev-master",
		"php-amqplib/rabbitmq-bundle" : "1.*",
		"hwi/oauth-bundle" : "~0.5",
		"friendsofsymfony/user-bundle" : "~2.0",
		"sensio/generator-bundle" : "~3.1.0",
		"leafo/scssphp" : "^0.6.6",
		"patchwork/jsqueeze" : "^2.0",
		"exercise/htmlpurifier-bundle" : "*",
		"doctrine/doctrine-migrations-bundle" : "^1.0",
		"symfony/http-kernel" : "~3.2.3",
		"knplabs/github-api": "^2.3",
		"php-http/guzzle6-adapter": "^1.1",
		"symfony/cache": "^3.2",
		"javiereguiluz/easyadmin-bundle": "^1.16",
		"beberlei/DoctrineExtensions": "^1.0"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
		]
	},
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
		"symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
		"symfony-assets-install" : "relative",
		"incenteev-parameters" : {
			"file" : "/opt/lc/site/app/config/parameters.yml"
		},
		"branch-alias" : {
			"dev-master" : "2.8-dev"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^6.2",
		"symfony/phpunit-bridge": "^3.3"
	}
}
